                    <label for="email" class="form__label">
                      <svg viewBox="0 0 370 50" preserveAspectRatio="none">
                        <path d="M0 50 L370 50 L370 0 L0 0 Z"></path>
                      </svg><span>Votre email</span>
                    </label>
                    <input type="email" name="email" id="email" class="form__control">
                  </div>
                  <div class="form__group">
                    <label for="subject" class="form__label">
                      <svg viewBox="0 0 370 50" preserveAspectRatio="none">
                        <path d="M0 50 L370 50 L370 0 L0 0 Z"></path>
                      </svg><span>Votre sujet</span>
                    </label>
                    <input type="text" name="subject" id="subject" class="form__control">
                  </div>
                  <div class="form__group textarea">
